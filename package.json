{
  "name": "lighthouse-dashboard",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "server",
    "client"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:server": "yarn --cwd server dev",
    "dev:client": "yarn --cwd client dev",
    "build": "run-p build:*",
    "build:server": "yarn --cwd server build",
    "build:client": "yarn --cwd client build"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.0.2"
  }
}
