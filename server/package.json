    "@types/mongoose": "^5.7.36",
    "@types/passport": "^1.0.4",
    "@types/passport-azure-ad": "^4.0.7",
    "@types/passport-jwt": "^3.0.3",
    "@types/socket.io": "^2.1.11",
    "@types/uuid": "^8.3.0",
    "body-parser": "^1.19.0",
    "dotenv": "*",
    "express": "^4.17.1",
